try:

except ValueError:
    print("부적절한 값을 인자로 받았습니다!(참조값이 없습니다!)")
except IndexError:
    print("인덱스 범위를 벗어났습니다!")
except SyntaxError:
    print("파이썬 문법 오류가 발생했습니다!")
except NameError:
    print("지역,전역 변수를 찾을 수 없습니다.")
except ZeroDivisionError:
    print("0으로 나눌 수 없습니다!")
except FileNotFoundError:
    print("파일이나 디렉터리가 없습니다!")
except TypeError:
    print("타입이 잘못됬습니다!")
except AttributeError:
    print("클레스의 객체 메서드나 속성을 잘못 호출 또는 대입했습니다!")
except KeyError:
    print("딕셔너리에 접근하려는 키 값이 없습니다!")
